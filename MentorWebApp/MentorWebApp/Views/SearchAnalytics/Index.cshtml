@model IEnumerable<MentorWebApp.Models.SearchAnalytic>

@{
    ViewData["Title"] = "Index";
}

<h2>Index</h2>

<p>
    <a asp-action="Create">Create New</a>
</p>
<table class="table">
    <thead>
        <tr>
                <th>
                    @Html.DisplayNameFor(model => model.NoOfResults)
                </th>
                <th>
                    @Html.DisplayNameFor(model => model.SucceedClicks)
                </th>
                <th>
                    @Html.DisplayNameFor(model => model.NoResultsCount)
                </th>
                <th>
                    @Html.DisplayNameFor(model => model.Count)
                </th>
                <th>
                    @Html.DisplayNameFor(model => model.ObjectId)
                </th>
            <th></th>
        </tr>
    </thead>
    <tbody>
@foreach (var item in Model) {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.NoOfResults)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.SucceedClicks)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.NoResultsCount)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Count)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.ObjectId)
            </td>
            <td>
                <a asp-action="Edit" asp-route-id="@item.Id">Edit</a> |
                <a asp-action="Details" asp-route-id="@item.Id">Details</a> |
                <a asp-action="Delete" asp-route-id="@item.Id">Delete</a>
            </td>
        </tr>
}
    </tbody>
</table>
